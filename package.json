{
  "name": "tpp-syntax-highlight",
  "displayName": "TPP syntax highlight",
  "description": "Syntax highlight for TPP language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tpp",
      "aliases": ["tpp", "tpp"],
      "extensions": [".tpp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tpp",
      "scopeName": "source.tpp",
      "path": "./syntaxes/tpp.tmLanguage.json"
    }]
  }
}
